<!-- Logout Modal-->
<div
  class="modal fade"
  id="logoutModal"
  tabindex="-1"
  role="dialog"
  aria-labelledby="exampleModalLabel"
  aria-hidden="true"
>
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title text-danger font-weight-bold" id="exampleModalLabel">ĐĂNG XUẤT</h5>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body">Bạn có chắc chắn muốn đăng xuất không ?</div>
      <div class="modal-footer">
        <a class="btn btn-primary" href="/login" onclick="logout()">Có</a>
        <button type="button" class="btn btn-danger" data-bs-dismiss="modal">
            Không
          </button>
      </div>
    </div>
  </div>
</div>
